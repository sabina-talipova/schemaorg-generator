{
    "name": "sabina-talipova/schemaorg-generator",
    "description": "Module for schemaorg generation",
    "type": "library",
    "autoload": {
        "psr-4": {
            "SabinaTalipova\\SchemaorgGenerator\\": "src/",
            "SabinaTalipova\\SchemaorgGenerator\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sabina Talipova",
            "email": "sabina@tasabina.co.nz"
        }
    ],
    "support": {
        "issues": "https://github.com/sabina-talipova/schemaorg-generator/issues"
    },
    "require": {
        "php": "^8.2",
        "silverstripe/framework": "^5.3",
        "silvershop/silverstripe-hasonefield": "^4",
        "symbiote/silverstripe-gridfieldextensions": "^4.1",
        "silverstripe/versioned": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
